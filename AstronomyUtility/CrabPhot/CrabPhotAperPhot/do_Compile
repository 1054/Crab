#cp CrabPhotAperPhot_mac ~/Cloud/Github/SpireLines/Softwares/ds9_mac/CrabPhotAperPhot_mac
#cp CrabPhotAperPhot_mac ~/Cloud/Github/Crab.Toolkit.PdBI/bin/bin_mac/CrabPhotAperPhot_mac

